<h1>Listing venues</h1>

<table>
  <tr>
    <th>Venue name</th>
    <th>Address</th>
    <th>Description</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Contact name</th>
    <th>Contact number</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @venues.each do |venue| %>
  <tr>
    <td><%= venue.venue_name %></td>
    <td><%= venue.address %></td>
    <td><%= venue.description %></td>
    <td><%= venue.latitude %></td>
    <td><%= venue.longitude %></td>
    <td><%= venue.contact_name %></td>
    <td><%= venue.contact_number %></td>
    <td><%= link_to 'Show', venue %></td>
    <td><%= link_to 'Edit', edit_venue_path(venue) %></td>
    <td><%= link_to 'Destroy', venue, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Venue', new_venue_path %>
